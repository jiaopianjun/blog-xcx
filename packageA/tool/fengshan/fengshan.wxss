  height: 100rpx;
}

.btns {
  display: flex;
  justify-content: center;
  border-radius: 12rpx;
  box-shadow: 0 16rpx 24rpx rgba(0, 0, 0, .08);
  background-color: hsla(0, 0%, 84.7%, .5);
}

.btns:before {
  content: " ";
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

.doll-guide {
  font-family: "sans-serif-regular", PingFangSC-Regular, PingFangTC-Regular, Roboto, Helvetica Neue, sans-serif;
  font-size: 30rpx;
  line-height: 46rpx;
  position: absolute;
  left: 114rpx;
  bottom: 0rpx;
  padding: 16rpx 80rpx 16rpx 24rpx;
  text-align: center;
  white-space: nowrap;
  color: #fff;
  border-radius: 12rpx;
  background-color: #13ce66;
  transition: opacity .3s ease-in;
  opacity: 0;
}

.doll-guide.show {
  transition: opacity .3s ease-out;
  opacity: 1;
}

.doll-guide:after {
  content: " ";
  position: absolute;
  left: -16rpx;
  top: 32rpx;
  width: 0;
  height: 0;
  border-color: #13ce66 #13ce66 transparent transparent;
  border-style: solid;
  border-width: 16rpx;
}

.btns-guide {
  font-family: "sans-serif-regular", PingFangSC-Regular, PingFangTC-Regular, Roboto, Helvetica Neue, sans-serif;
  font-size: 24rpx;
  line-height: 40rpx;
  position: absolute;
  top: -56rpx;
  color: rgba(0, 0, 0, .4);
  transition: opacity .3s ease-in;
  opacity: 0;
  white-space: pre;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 100%;
}

.btns-guide.show {
  transition: opacity .3s ease-out;
  opacity: 1;
}

