page {
  overflow: hidden;
  background: linear-gradient(#ebebeb, #fcfcfc);
}
.container {
  overflow: visible;
}
.sunny-doll-container {
  position: absolute;
  top: 0;
  left: 48rpx;
  z-index: 12;
}

.sunny-doll {
  position: relative;
  margin-top: 200rpx;
  width: 36rpx;
  height: 72rpx;
  transform: rotate(10deg);
}

.shutters {
  transform: translateY(0);
  opacity: 0;
  top: 0;
  height: 100vh;
  width: 100vw;
  z-index: 11;
  margin-top: -100vh;
  animation-fill-mode: forwards;
  animation-name: slideup;
  animation-duration: .3s;
  background-color: hsla(0, 0%, 100%, .98);
}

.shutters,
.shutters:after {
  position: absolute;
  left: 0;
  right: 0;
}

.shutters:after {
  content: " ";
  display: block;
  bottom: 0;
  height: 34rpx;
  background: linear-gradient(#525252, #292929);
}

.shutters-footer {
  position: fixed;
  bottom: 34rpx;
  padding-bottom: 30px;
  left: 0;
  right: 0;
  background-color: hsla(0, 0%, 100%, .64);
}

.shutters-ready {
  opacity: 1;
}

.shutters scroll-view {
  height: 100%;
}

.close-chutters {
  font-family: "sans-serif-semibold", PingFangSC-Semibold, PingFangTC-Semibold, Roboto, Helvetica Neue, sans-serif;
  font-size: 30rpx;
  line-height: 46rpx;
  padding: 16rpx;
  color: rgba(0, 0, 0, .72);
}

.slide-down {
  transform-origin: center bottom;
  animation-name: bounce;
  animation-duration: .6s;
  animation-timing-function: linear;
  transform: translateZ(0);
}

.instructions-title {
  font-family: "sans-serif-semibold", PingFangSC-Semibold, PingFangTC-Semibold, Roboto, Helvetica Neue, sans-serif;
  font-size: 34rpx;
  line-height: 50rpx;
  color: #000;
  padding-top: 260rpx;
  margin: 0 auto;
  text-align: center;
}

.instructions-content {
  font-family: "sans-serif-regular", PingFangSC-Regular, PingFangTC-Regular, Roboto, Helvetica Neue, sans-serif;
  font-size: 30rpx;
  line-height: 46rpx;
  color: rgba(0, 0, 0, .87);
  margin: 32rpx 80rpx 0;
}

.cells {
  padding-bottom: 200rpx;
}

.cells-title {
  font-family: "sans-serif-semibold", PingFangSC-Semibold, PingFangTC-Semibold, Roboto, Helvetica Neue, sans-serif;
  font-size: 26rpx;
  line-height: 42rpx;
  padding: 70rpx 80rpx 16rpx;
