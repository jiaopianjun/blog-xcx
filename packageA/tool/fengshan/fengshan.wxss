}

.light-lv1,
.light-lv2,
.light-lv3 {
  display: none;
  position: absolute;
  width: 80rpx;
  height: 84rpx;
  top: 128rpx;
}

.lv-1 .light-lv1,
.lv-2 .light-lv2,
.lv-3 .light-lv3 {
  display: block;
}

.back,
.blades,
.front {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
}

.back {
  z-index: 1;
}

.front {
  z-index: 3;
}

.blades {
  z-index: 2;
  transition-property: transform;
  transition-property: transform, -webkit-transform;
  transition-duration: 60s;
  transition-timing-function: linear !important;
}

.blades.up-1,
.blades.up-2,
.blades.up-3 {
  transition-duration: 1s;
  transition-timing-function: ease-in !important;
}

.blades.down-1 {
  transition-duration: 2s;
  transition-timing-function: cubic-bezier(.45, .33, 0, .96) !important;
}

.blades.down-2 {
  transition-duration: 3s;
  transition-timing-function: cubic-bezier(.45, .33, 0, .96) !important;
}

.blades.down-3 {
  transition-duration: 4s;
  transition-timing-function: cubic-bezier(.45, .33, 0, .96) !important;
}

.control-panel {
  flex-direction: row;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
