<view class="container">
  <image src="data:image/png;base64," class="pic"/>
  <text class="tip">请点击下方按钮，并在后续的页面中点击“允许”</text>
  <button class="btn" open-type="getPhoneNumber" bindgetphonenumber="handleTap">确认授权</button>
  <view class="shade" bindtap="hideModal" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>
  <view class="container">
    <view class="modal" wx:if="{{ showModal }}">
      <view class="title">{{ modalTitle }}</view>
      <view class="content">{{ modalContent }}</view>
      <button open-type="launchApp" app-parameter="{{ext}}" class="btn" plain="true" bindtap="clickBtn">返回小程序</button>
    </view>
  </view>
</view>
