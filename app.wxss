  color: var(--normal-text-color);
}

._clear {
  clear: both;
}

._hover {
  opacity: .5;
}

._hover-have-bg {
  background: var(--module-bg-color);
  opacity: .5;
}

._hidden {
  opacity: 0;
}

._hidden,._show {
  transition: opacity .3s;
}

._show {
  opacity: 1;
}